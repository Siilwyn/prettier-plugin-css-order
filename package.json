{
  "name": "prettier-plugin-css-order",
  "version": "2.1.2",
  "description": "Sort CSS declarations in a certain order.",
  "type": "module",
  "main": "./src/main.mjs",
  "files": [
    "src/*.mjs",
    "!src/*.test.mjs"
  ],
  "scripts": {
    "test": "node src/main.test.mjs",
    "lint": "prettier src --check",
    "format": "prettier src --write",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "css-declaration-sorter": "^7.1.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9"
  },
  "peerDependencies": {
    "postcss": "8.x",
    "prettier": "3.x"
  },
  "devDependencies": {
    "postcss": "^8.4.47",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Siilwyn/prettier-plugin-css-order.git"
  },
  "author": "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)",
  "license": "ISC",
  "keywords": [
    "prettier",
    "prettier-plugin",
    "css",
    "scss",
    "less",
    "declaration",
    "sorter",
    "property",
    "order"
  ]
}
